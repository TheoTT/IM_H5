{
  "compilerOptions": {
    "declaration": false,
    "module": "ESNext",
    "target": "ES5",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom",
      "scripthost"
    ],
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "newLine": "LF",
    "moduleResolution": "node",
    "importHelpers": true,
    "pretty": true,
    "noUnusedLocals": true
  },
  "include": [
    "./src/index.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
